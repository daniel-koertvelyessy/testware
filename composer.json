{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2",
        "elibyy/tcpdf-laravel": "^11.1.0",
        "guzzlehttp/guzzle": "^7.7.0",
        "kyslik/column-sortable": "^6.6.0",
        "laravel/framework": "^v10.48.23",
        "laravel/helpers": "^v1.7.0",
        "laravel/tinker": "^v2.9.0",
        "laravel/ui": "^v4.5.1",
        "simplesoftwareio/simple-qrcode": "^4.2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.13.5",
        "barryvdh/laravel-ide-helper": "^v3.0.0",
        "fakerphp/faker": "^1.16.1",
        "mockery/mockery": "^1.4.31",
        "nunomaduro/collision": "^v7.10.0",
        "phpunit/phpunit": "^10.5.20",
        "spatie/laravel-ignition": "^2.7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": ["app/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
